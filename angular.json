{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"nazox": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/nazox",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"assets": [
							"src/favicon.ico",
							"src/assets"
						],
						"styles": [
							"node_modules/angular-archwizard/styles/archwizard.css",
							"./node_modules/ngx-ui-switch/ui-switch.component.css",
							"src/assets/scss/bootstrap.scss",
							"src/assets/scss/app.scss",
							"src/styles.scss"
						],
						"scripts": [],
						"allowedCommonJsDependencies": [
							"apexcharts",
							"dropzone",
							"sweetalert2",
							"leaflet",
							"@ckeditor/ckeditor5-build-classic",
							"bs-custom-file-input"
						],
						"vendorChunk": true,
						"extractLicenses": false,
						"buildOptimizer": false,
						"sourceMap": true,
						"optimization": false,
						"namedChunks": true
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							]
						}
					},
					"defaultConfiguration": ""
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "nazox:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "nazox:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "nazox:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.spec.json",
						"karmaConfig": "karma.conf.js",
						"assets": [
							"src/favicon.ico",
							"src/assets"
						],
						"styles": [
							"src/styles.scss"
						],
						"scripts": []
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "e2e/protractor.conf.js",
						"devServerTarget": "nazox:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "nazox:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": [
							"src/**/*.ts",
							"src/**/*.html"
						]
					}
				}
			}
		},
		"cd-shell": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/cd-shell",
			"sourceRoot": "projects/cd-shell/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/cd-shell",
						"index": "projects/cd-shell/src/index.html",
						"main": "projects/cd-shell/src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/cd-shell/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/cd-shell/src/favicon.ico",
							"projects/cd-shell/src/assets"
						],
						"styles": [
							"node_modules/angular-archwizard/styles/archwizard.css",
							"./node_modules/ngx-ui-switch/ui-switch.component.css",
							"projects/cd-shell/src/assets/scss/bootstrap.scss",
							"projects/cd-shell/src/assets/scss/app.scss",
							"projects/cd-shell/src/styles.scss"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "cd-shell:build:production"
						},
						"development": {
							"browserTarget": "cd-shell:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "cd-shell:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/cd-shell/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/cd-shell/src/favicon.ico",
							"projects/cd-shell/src/assets"
						],
						"styles": [
							"projects/cd-shell/src/styles.scss"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": [
							"projects/cd-shell/**/*.ts",
							"projects/cd-shell/**/*.html"
						]
					}
				}
			}
		},
		"cd-user": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/cd-user",
			"sourceRoot": "projects/cd-user/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/cd-user",
						"index": "projects/cd-user/src/index.html",
						"main": "projects/cd-user/src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/cd-user/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/cd-user/src/favicon.ico",
							"projects/cd-user/src/assets"
						],
						"styles": [
							"node_modules/angular-archwizard/styles/archwizard.css",
							"./node_modules/ngx-ui-switch/ui-switch.component.css",
							"projects/cd-user/src/assets/scss/bootstrap.scss",
							"projects/cd-user/src/assets/scss/app.scss",
							"projects/cd-user/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/cd-user/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/cd-user/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "cd-user:build:production",
							"extraWebpackConfig": "projects/cd-user/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "cd-user:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"proxyConfig": "projects/cd-user/src/proxy.conf.json",
						"port": 4407,
						"publicHost": "http://cd-user-25:4407",
						"extraWebpackConfig": "projects/cd-user/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "cd-user:build",
						"extraWebpackConfig": "projects/cd-user/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/cd-user/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/cd-user/src/favicon.ico",
							"projects/cd-user/src/assets"
						],
						"styles": [
							"projects/cd-user/src/styles.scss"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": [
							"projects/cd-user/**/*.ts",
							"projects/cd-user/**/*.html"
						]
					}
				}
			}
		}
	},
	"cli": {
		"analytics": "915204d4-f209-4eda-93c6-37a8757fd25c",
		"schematicCollections": [
			"@angular-eslint/schematics"
		]
	}
}